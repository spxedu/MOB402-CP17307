<%- include('../inc/top') %>

<h1>List sp</h1>

<table class="table table-bordered table-striped">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Price</th>
        <th>Desc</th>
        <th>Thể loại</th>
    </tr>
    <% listSP.forEach( (objSP) => { %>
        
        <tr>
            <td> 
                <a href="/sp/edit/<%= objSP._id %>">  <%= objSP._id %> </a>
               
             </td>
            <td> <%= objSP.name %> </td>
            <td> <%= objSP.price %> </td>
            <td> <%= objSP.description %></td>
            <td> <%= objSP.id_theloai.name %></td>
        </tr>

   <% }); %>
    
</table>




<%- include('../inc/bottom') %>